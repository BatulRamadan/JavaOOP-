package encapsulation_01;

public class BATUL 
{
private int a;
private int b;
private String s; 

public BATUL()
{
    
a=5;
b=20;

}

public void setA(int a)
{
    if(a%2==0)
        
     this.a=a;
    
    else
    {
    this.a=-3;
    System.out.println("The number is not even");
    
    }
}
public int getA()
{
 return a;   
}
public void setB(int b)
{
    if(b >= 15)
    this.b=b;
    else
    {
    System.out.println("Sorry");
    }

}
public int getB()
{
    return b;
}
public void setS(String s)
{
this.s=s;
}
public String getS()
{
return s;
}
}


